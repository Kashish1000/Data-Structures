#include<iostream>
#include<string>
#include<algorithm>
#include<bits/stdc++.h>
using namespace std;

void permute(string str)
{
    sort(str.begin(),str.end()); // sort() function sorts the given container 
    
    do{
        cout<<str<<endl;
    }while( next_permutation( str.begin() , str.end() ) );

    
}
int main()
{
    string str="AbC";
    permute(str);
    
    return(0);
}
